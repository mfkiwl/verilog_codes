#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x564bdf6d95b0 .scope module, "clock_div_tb" "clock_div_tb" 2 1;
 .timescale 0 0;
v0x564bdf6f9c70_0 .var "clk_in", 0 0;
v0x564bdf6f9d40_0 .net "clk_out", 0 0, L_0x564bdf69e790;  1 drivers
S_0x564bdf6d9730 .scope module, "clk_divider" "clock_divider" 2 6, 3 1 0, S_0x564bdf6d95b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk_in"
    .port_info 1 /OUTPUT 1 "clk_out"
P_0x564bdf6d9900 .param/l "N_DIVISIION" 0 3 2, +C4<00000000000000000000000000010000>;
L_0x564bdf69e790 .functor BUFZ 1, v0x564bdf6f9b50_0, C4<0>, C4<0>, C4<0>;
v0x564bdf6d99a0_0 .net "clk_in", 0 0, v0x564bdf6f9c70_0;  1 drivers
v0x564bdf6f99a0_0 .net "clk_out", 0 0, L_0x564bdf69e790;  alias, 1 drivers
v0x564bdf6f9a60_0 .var "counter", 3 0;
v0x564bdf6f9b50_0 .var "r_clk", 0 0;
E_0x564bdf6d3980 .event posedge, v0x564bdf6d99a0_0;
    .scope S_0x564bdf6d9730;
T_0 ;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x564bdf6f9a60_0, 0, 4;
    %end;
    .thread T_0;
    .scope S_0x564bdf6d9730;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x564bdf6f9b50_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x564bdf6d9730;
T_2 ;
    %wait E_0x564bdf6d3980;
    %load/vec4 v0x564bdf6f9a60_0;
    %pad/u 32;
    %cmpi/e 7, 0, 32;
    %jmp/0xz  T_2.0, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x564bdf6f9a60_0, 0;
    %load/vec4 v0x564bdf6f9b50_0;
    %inv;
    %assign/vec4 v0x564bdf6f9b50_0, 0;
    %jmp T_2.1;
T_2.0 ;
    %load/vec4 v0x564bdf6f9a60_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x564bdf6f9a60_0, 0;
T_2.1 ;
    %jmp T_2;
    .thread T_2;
    .scope S_0x564bdf6d95b0;
T_3 ;
    %vpi_call 2 10 "$from_myhdl", v0x564bdf6f9c70_0 {0 0 0};
    %vpi_call 2 11 "$to_myhdl", v0x564bdf6f9d40_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x564bdf6d95b0;
T_4 ;
    %vpi_call 2 15 "$dumpfile", "clk_div.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "clock_div_tb.v";
    "clock_divider.v";
